<br>
<div class = "landing-div">
	<h1>Welcome!</h1>
	<br>
	<h2>Please <%=link_to "Log In", login_path%> or <%=link_to "Sign Up", signup_path%> To Continue</h2>
<br>
<br>
	<%= form_for(:session, url: login_path) do %>
	 <p>
	 	<%= label_tag :username %>
	 	<br>
	 	<%= text_field_tag :username %>
	 </p>
	 <p>
	 	<%= label_tag :password %>
	 	<br>
	 	<%= password_field_tag :password %>
	 </p>
	 <p>
	 	<%= submit_tag 'Log in' %>
	 </p>
	<% end %>

</div>
<br>
